<div class="container">


    <div>
    <br>
        <% if flash[:message] %>
        <p style="color:#FF0000";><%= flash[:message] %></p>
        <% end %>
    </div>

    <br></br>
    <h3>Other Events You Have RSVP'd To:</h3>
    <br>
    
    
        <% @users_events.each do |ue| %>
            <%# byebug %>
            <% if ue.user_id == current_user.id %>
                <%= ue.event[:title] %> -<%= ue.comment %><br>
                <%= button_to "Delete RSVP", users_event_path(ue), method: :delete %><br>
            <% end %>
        <% end %>
   


    <br></br>
    <br></br>

    <div>
        <p> You're logged in as: <%= link_to current_user.username, user_path(current_user) %><br>
        <%= link_to "Log Out", logout_path %></p><br> 
    </div>

</div>

