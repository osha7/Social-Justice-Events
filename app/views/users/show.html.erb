<h2> Hello <%= @user.name %> </h2> 

<%= link_to "See All Events", '/events' %>
<br>
<%= link_to "Create New Event", '/events/new' %>

<br></br>

<h5>Events You're Going To:</h5>
<%# byebug %>
    <% @user.events.each do |event| %>
        <%# byebug %>
        <%= link_to event.title, event_path(event) %> -
        <% ed = event.date %>
        <%= ed.strftime("%m/%d/%Y") %> -
        <% et = event.time %>
        <%= et.strftime("%I:%M %p") %>
        <br>
    <% end %>

<br></br>

<h5>Your Events as Admin:</h5>
<%# byebug %>
    <% if @user.users_events.where(admin: true) %>
        <% @user.users_events.where(admin: true).each do |event| %>
            <%# byebug %>
            <%= link_to Event.all.find(event.event_id).title, event_path(event.event_id) %> -
            <% ed = Event.all.find(event.event_id).date %>
            <%= ed.strftime("%m/%d/%Y") %>
            <%# et = Event.all.find(event.event_id).time %>
            <%#= et.strftime("%I:%M %p") %>
            <br>        
        <% end %>
    <% end %>

 




