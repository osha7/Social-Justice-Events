<h2> Hello <%= @user.name %> </h2> 

<%= link_to "See All Events", '/events' %>
<br>
<%= link_to "Create New Event", '/events/new' %>

<br></br>

<h5>Events You're Going To:</h5>
<%# byebug %>
    <% @user.events.each do |event| %>
        <%# byebug %>
        <%= link_to event.title, event_path(event) %> -
        <% ed = event.date %>
        <%= ed.strftime("%m/%d/%Y") %> -
        <% et = event.time %>
        <%= et.strftime("%I:%M %p") %>
        <br>
    <% end %>

<br></br>

<h5>Your Events as Admin:</h5>
    <% if @user.users_events.where(admin: true) %>
        <% @user.users_events.where(admin: true).each do |event| %>
            <%# byebug %>
            <%= link_to event.id, event_path(event) %> -
            <%# ed = event.date %>
            <%#= ed.strftime("%m/%d/%Y") %> -
            <%# et = event.time %>
            <%#= et.strftime("%I:%M %p") %>
            <br>        
        <% end %>
    <% end %>






<br></br>
<br></br>
<p> You're logged in as: <%= link_to current_user.username, user_path(current_user) %><br>
<%= link_to "Log Out", logout_path %></p><br> 